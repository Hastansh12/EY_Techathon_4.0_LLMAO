{
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 1: \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940\u091a\u093e \u092a\u0930\u093f\u091a\u092f": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 \u0936\u093f\u092b\u093e\u0930\u0938\u0915\u0930\u094d\u0924\u094d\u092f\u093e \u092a\u094d\u0930\u0923\u093e\u0932\u0940\u0902\u091a\u0947 \u0935\u093f\u0939\u0902\u0917\u093e\u0935\u0932\u094b\u0915\u0928 \u092a\u094d\u0930\u0926\u093e\u0928 \u0915\u0930\u0924\u0947, \u0924\u094d\u092f\u093e\u0902\u091a\u094d\u092f\u093e \u0909\u0926\u094d\u0926\u0947\u0936, \u092a\u094d\u0930\u0915\u093e\u0930 \u0906\u0923\u093f \u0918\u091f\u0915\u093e\u0902\u0938\u0939.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 2: \u0938\u093e\u092e\u0917\u094d\u0930\u0940-\u0906\u0927\u093e\u0930\u093f\u0924 \u092b\u093f\u0932\u094d\u091f\u0930\u093f\u0902\u0917": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 \u0936\u093f\u092b\u093e\u0930\u0938\u0915\u0930\u094d\u0924\u093e \u092a\u094d\u0930\u0923\u093e\u0932\u0940\u0902\u092e\u0927\u094d\u092f\u0947 \u0935\u093e\u092a\u0930\u0932\u094d\u092f\u093e \u091c\u093e\u0923\u093e\u0930\u094d\u200d\u092f\u093e \u0938\u093e\u092e\u0917\u094d\u0930\u0940-\u0906\u0927\u093e\u0930\u093f\u0924 \u092b\u093f\u0932\u094d\u091f\u0930\u093f\u0902\u0917 \u0924\u0902\u0924\u094d\u0930\u093e\u091a\u0947 \u0905\u0928\u094d\u0935\u0947\u0937\u0923 \u0915\u0930\u0924\u0947, \u091c\u094d\u092f\u093e\u092e\u0927\u094d\u092f\u0947 \u0935\u093e\u092a\u0930\u0915\u0930\u094d\u0924\u094d\u092f\u093e\u0928\u0947 \u092a\u0942\u0930\u094d\u0935\u0940 \u0906\u0935\u0921\u0932\u0947\u0932\u094d\u092f\u093e \u0935\u0938\u094d\u0924\u0942\u0902\u092a\u094d\u0930\u092e\u093e\u0923\u0947\u091a \u0936\u093f\u092b\u093e\u0930\u0938 \u0915\u0930\u0923\u0947 \u0938\u092e\u093e\u0935\u093f\u0937\u094d\u091f \u0905\u0938\u0924\u0947.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 3: \u0938\u0939\u092f\u094b\u0917\u0940 \u092b\u093f\u0932\u094d\u091f\u0930\u093f\u0902\u0917": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 \u0938\u0939\u092f\u094b\u0917\u0940 \u092b\u093f\u0932\u094d\u091f\u0930\u093f\u0902\u0917\u0935\u0930 \u0932\u0915\u094d\u0937 \u0915\u0947\u0902\u0926\u094d\u0930\u093f\u0924 \u0915\u0930\u0924\u0947, \u0905\u0928\u0947\u0915 \u0935\u093e\u092a\u0930\u0915\u0930\u094d\u0924\u094d\u092f\u093e\u0902\u0915\u0921\u0942\u0928 \u092a\u094d\u0930\u093e\u0927\u093e\u0928\u094d\u092f\u0947 \u090f\u0915\u0924\u094d\u0930\u093f\u0924 \u0915\u0930\u0942\u0928 \u0935\u093e\u092a\u0930\u0915\u0930\u094d\u0924\u094d\u092f\u093e\u091a\u094d\u092f\u093e \u0939\u093f\u0924\u0938\u0902\u092c\u0902\u0927\u093e\u0902\u092c\u0926\u094d\u0926\u0932 \u0938\u094d\u0935\u092f\u0902\u091a\u0932\u093f\u0924 \u0905\u0902\u0926\u093e\u091c (\u092b\u093f\u0932\u094d\u091f\u0930\u093f\u0902\u0917) \u092c\u0928\u0935\u0923\u094d\u092f\u093e\u091a\u0940 \u090f\u0915 \u0932\u094b\u0915\u092a\u094d\u0930\u093f\u092f \u092a\u0926\u094d\u0927\u0924 \u0906\u0939\u0947.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 4: \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940\u0938\u093e\u0920\u0940 \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u0915\u0928 \u092e\u0947\u091f\u094d\u0930\u093f\u0915\u094d\u0938": "\u092f\u093e \u092e\u0949\u0921\u094d\u092f\u0942\u0932\u092e\u0927\u094d\u092f\u0947 \u0905\u091a\u0942\u0915\u0924\u093e, \u0938\u0941\u0938\u094d\u092a\u0937\u094d\u091f\u0924\u093e, \u0930\u093f\u0915\u0949\u0932 \u0906\u0923\u093f F1 \u0938\u094d\u0915\u094b\u0905\u0930\u0938\u0939 \u0936\u093f\u092b\u093e\u0930\u0938 \u0915\u0930\u0923\u093e\u0930\u094d\u200d\u092f\u093e \u0938\u093f\u0938\u094d\u091f\u0940\u092e\u091a\u0940 \u0915\u093e\u0930\u094d\u092f\u0915\u094d\u0937\u092e\u0924\u093e \u092e\u094b\u091c\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0935\u093e\u092a\u0930\u0932\u094d\u092f\u093e \u091c\u093e\u0923\u093e\u0930\u094d\u200d\u092f\u093e \u0935\u093f\u0935\u093f\u0927 \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u0915\u0928 \u092e\u0947\u091f\u094d\u0930\u093f\u0915\u094d\u0938\u091a\u093e \u0938\u092e\u093e\u0935\u0947\u0936 \u0906\u0939\u0947.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 5: \u092e\u0945\u091f\u094d\u0930\u093f\u0915\u094d\u0938 \u092b\u0945\u0915\u094d\u091f\u0930\u093e\u092f\u091d\u0947\u0936\u0928": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 \u092e\u0945\u091f\u094d\u0930\u093f\u0915\u094d\u0938 \u092b\u0945\u0915\u094d\u091f\u0930\u093e\u092f\u091d\u0947\u0936\u0928\u091a\u093e \u0936\u094b\u0927 \u0918\u0947\u0924\u0947, \u0936\u093f\u092b\u093e\u0930\u0938\u0915\u0930\u094d\u0924\u093e \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u0924\u092f\u093e\u0930 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u090f\u0915 \u0932\u094b\u0915\u092a\u094d\u0930\u093f\u092f \u0924\u0902\u0924\u094d\u0930 \u091c\u0947 \u0935\u093e\u092a\u0930\u0915\u0930\u094d\u0924\u093e-\u0906\u092f\u091f\u092e \u092a\u0930\u0938\u094d\u092a\u0930\u0938\u0902\u0935\u093e\u0926 \u092e\u0945\u091f\u094d\u0930\u093f\u0915\u094d\u0938\u091a\u0947 \u0928\u093f\u092e\u094d\u0928 \u092e\u093f\u0924\u0940\u092f \u092e\u0945\u091f\u094d\u0930\u093f\u0915\u094d\u0938\u092e\u0927\u094d\u092f\u0947 \u0935\u093f\u0918\u091f\u0928 \u0915\u0930\u0942\u0928 \u0915\u093e\u0930\u094d\u092f \u0915\u0930\u0924\u0947."
}