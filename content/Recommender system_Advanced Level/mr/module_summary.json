{
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 1: \u092a\u094d\u0930\u0917\u0924 \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u0905\u0932\u094d\u0917\u094b\u0930\u093f\u0926\u092e": "\u092f\u093e \u092e\u0949\u0921\u094d\u092f\u0942\u0932\u092e\u0927\u094d\u092f\u0947 \u092e\u0945\u091f\u094d\u0930\u093f\u0915\u094d\u0938 \u092b\u0945\u0915\u094d\u091f\u0930\u093e\u092f\u091d\u0947\u0936\u0928, \u0921\u0940\u092a \u0932\u0930\u094d\u0928\u093f\u0902\u0917 \u0906\u0923\u093f \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u0924\u092f\u093e\u0930 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0939\u093e\u092f\u092c\u094d\u0930\u0940\u0921 \u092e\u0949\u0921\u0947\u0932\u094d\u0938 \u092f\u093e\u0938\u093e\u0930\u0916\u094d\u092f\u093e \u092a\u094d\u0930\u0917\u0924 \u0924\u0902\u0924\u094d\u0930\u093e\u0902\u091a\u093e \u0938\u092e\u093e\u0935\u0947\u0936 \u0905\u0938\u0947\u0932.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 2: \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940\u091a\u0947 \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u0915\u0928 \u0906\u0923\u093f \u092a\u094d\u0930\u092e\u093e\u0923\u0940\u0915\u0930\u0923": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 A/B \u091a\u093e\u091a\u0923\u0940 \u0906\u0923\u093f \u0915\u094d\u0930\u0949\u0938-\u0935\u094d\u0939\u0945\u0932\u093f\u0921\u0947\u0936\u0928\u0938\u0939 \u0936\u093f\u092b\u093e\u0930\u0938 \u0915\u0930\u0923\u093e\u0930\u094d\u200d\u092f\u093e \u0938\u093f\u0938\u094d\u091f\u092e\u091a\u094d\u092f\u093e \u0915\u093e\u0930\u094d\u092f\u0915\u094d\u0937\u092e\u0924\u0947\u091a\u0947 \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u0915\u0928 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u092a\u094d\u0930\u0917\u0924 \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u0915\u0928 \u092e\u0947\u091f\u094d\u0930\u093f\u0915\u094d\u0938 \u0906\u0923\u093f \u092a\u094d\u0930\u092e\u093e\u0923\u0940\u0915\u0930\u0923 \u0924\u0902\u0924\u094d\u0930\u093e\u0902\u0935\u0930 \u0932\u0915\u094d\u0937 \u0915\u0947\u0902\u0926\u094d\u0930\u093f\u0924 \u0915\u0930\u0947\u0932.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 3: \u0935\u0948\u092f\u0915\u094d\u0924\u093f\u0915\u0930\u0923 \u0906\u0923\u093f \u0938\u0902\u0926\u0930\u094d\u092d-\u091c\u093e\u0917\u0930\u0942\u0915 \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 \u0935\u093e\u092a\u0930\u0915\u0930\u094d\u0924\u093e \u092a\u094d\u0930\u093e\u0927\u093e\u0928\u094d\u092f\u0947, \u0935\u0930\u094d\u0924\u0928 \u0906\u0923\u093f \u0938\u094d\u0925\u093e\u0928 \u0906\u0923\u093f \u0935\u0947\u0933 \u092f\u093e\u0938\u093e\u0930\u0916\u094d\u092f\u093e \u0938\u0902\u0926\u0930\u094d\u092d\u093f\u0924 \u092e\u093e\u0939\u093f\u0924\u0940\u0935\u0930 \u0906\u0927\u093e\u0930\u093f\u0924 \u0936\u093f\u092b\u093e\u0930\u0938\u0940 \u0935\u0948\u092f\u0915\u094d\u0924\u093f\u0915\u0943\u0924 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u092a\u094d\u0930\u0917\u0924 \u092a\u0926\u094d\u0927\u0924\u0940\u0902\u091a\u093e \u0905\u092d\u094d\u092f\u093e\u0938 \u0915\u0930\u0947\u0932.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 4: \u0938\u094d\u0915\u0947\u0932\u0947\u092c\u0932 \u0906\u0923\u093f \u0935\u093f\u0924\u0930\u093f\u0924 \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u092f\u093e \u092e\u0949\u0921\u094d\u092f\u0942\u0932\u092e\u0927\u094d\u092f\u0947, \u0935\u093f\u0926\u094d\u092f\u093e\u0930\u094d\u0925\u0940 \u0938\u092e\u093e\u0902\u0924\u0930 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e, \u0935\u093f\u0924\u0930\u093f\u0924 \u0938\u0902\u0917\u0923\u0928 \u0906\u0923\u093f \u0915\u094d\u0932\u093e\u0909\u0921-\u0906\u0927\u093e\u0930\u093f\u0924 \u0938\u094b\u0932\u094d\u092f\u0942\u0936\u0928\u094d\u0938\u0938\u0939 \u092e\u094b\u0920\u094d\u092f\u093e \u092a\u094d\u0930\u092e\u093e\u0923\u093e\u0924, \u0935\u093f\u0924\u0930\u093f\u0924 \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u0924\u092f\u093e\u0930 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u092a\u094d\u0930\u0917\u0924 \u0924\u0902\u0924\u094d\u0930\u093e\u0902\u092c\u0926\u094d\u0926\u0932 \u0936\u093f\u0915\u0924\u0940\u0932.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 5: \u0928\u0948\u0924\u093f\u0915 \u0906\u0923\u093f \u0909\u091a\u093f\u0924 \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 \u092a\u0942\u0930\u094d\u0935\u093e\u0917\u094d\u0930\u0939 \u0936\u094b\u0927\u0923\u0947 \u0906\u0923\u093f \u0915\u092e\u0940 \u0915\u0930\u0923\u094d\u092f\u093e\u091a\u094d\u092f\u093e \u0927\u094b\u0930\u0923\u093e\u0902\u0938\u0939 \u0936\u093f\u092b\u093e\u0930\u0938 \u0915\u0930\u0923\u093e\u0930\u094d\u200d\u092f\u093e \u092a\u094d\u0930\u0923\u093e\u0932\u0940\u0902\u092e\u0927\u0940\u0932 \u0928\u0948\u0924\u093f\u0915 \u0935\u093f\u091a\u093e\u0930, \u0928\u093f\u0937\u094d\u092a\u0915\u094d\u0937\u0924\u093e \u0906\u0923\u093f \u092a\u093e\u0930\u0926\u0930\u094d\u0936\u0915\u0924\u0947\u0936\u0940 \u0938\u0902\u092c\u0902\u0927\u093f\u0924 \u092a\u094d\u0930\u0917\u0924 \u0935\u093f\u0937\u092f\u093e\u0902\u0928\u093e \u0938\u0902\u092c\u094b\u0927\u093f\u0924 \u0915\u0930\u0947\u0932.",
    "\u092e\u0949\u0921\u094d\u092f\u0942\u0932 6: \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940\u0938\u093e\u0920\u0940 \u092e\u091c\u092c\u0941\u0924\u0940\u0915\u0930\u0923 \u0936\u093f\u0915\u094d\u0937\u0923": "\u0939\u0947 \u092e\u0949\u0921\u094d\u092f\u0942\u0932 \u092e\u0932\u094d\u091f\u0940-\u0906\u0930\u094d\u092e\u094d\u0921 \u092c\u0945\u0928\u094d\u0921\u093f\u091f \u0905\u0932\u094d\u0917\u094b\u0930\u093f\u0926\u092e \u0906\u0923\u093f \u0938\u0902\u0926\u0930\u094d\u092d\u093f\u0924 \u092c\u0901\u0921\u093f\u091f\u094d\u0938\u0938\u0939, \u0905\u0928\u0941\u0915\u0942\u0932\u0940 \u0906\u0923\u093f \u092a\u0930\u0938\u094d\u092a\u0930\u0938\u0902\u0935\u093e\u0926\u0940 \u0936\u093f\u092b\u093e\u0930\u0938 \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u0924\u092f\u093e\u0930 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u092e\u091c\u092c\u0941\u0924\u0940\u0915\u0930\u0923 \u0936\u093f\u0915\u094d\u0937\u0923\u093e\u091a\u094d\u092f\u093e \u092a\u094d\u0930\u0917\u0924 \u0905\u0928\u0941\u092a\u094d\u0930\u092f\u094b\u0917\u093e\u0902\u091a\u0947 \u0905\u0928\u094d\u0935\u0947\u0937\u0923 \u0915\u0930\u0947\u0932."
}